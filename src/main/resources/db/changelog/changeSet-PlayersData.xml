<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.4.xsd">
    <changeSet author="Swastik" id="swastik.dash1@gmail.com">
        <createTable tableName="player_data">
            <column autoIncrement="true" name="id" type="INTEGER">
                <constraints nullable="false" primaryKey="true"/>
            </column>
            <column name="name" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="image" type="VARCHAR(250)">
                <constraints nullable="false"/>
            </column>
            <column name="data__id" type="INTEGER">
                <constraints nullable="false"/>
            </column>
            <column name="is_sold" type="BOOLEAN">
                <constraints nullable="false"/>
            </column>
            <column name="at_price" type="INTEGER">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>